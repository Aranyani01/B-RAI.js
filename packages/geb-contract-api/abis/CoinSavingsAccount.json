        "methods": {
            "addAuthorization(address)": {
                "params": { "account": "Account to add auth to" }
            },
            "deposit(uint256)": {
                "params": {
                    "wad": "Amount of coins to deposit (expressed as an 18 decimal number). 'wad' will be multiplied by 'accumulatedRate' (27 decimals) to result in a correct amount of internal coins transferred"
                }
            },
            "modifyParameters(bytes32,address)": {
                "params": {
                    "addr": "New value for the parameter",
                    "parameter": "The name of the parameter modified"
                }
            },
            "modifyParameters(bytes32,uint256)": {
                "params": {
                    "data": "New value for the parameter",
                    "parameter": "The name of the parameter modified"
                }
            },
            "removeAuthorization(address)": {
                "params": { "account": "Account to remove auth from" }
            },
            "updateAccumulatedRate()": {
                "details": "We return early if 'latestUpdateTime' is smaller or equal to block.timestamp. When the savings rate is positive, we create unbacked debt for the accounting engine and issue new coins for this contract"
            },
            "withdraw(uint256)": {
                "params": {
                    "wad": "Amount of coins to withdraw (expressed as an 18 decimal number). 'wad' will be multiplied by 'accumulatedRate' (27 decimals) to result in a correct amount of internal coins transferred"
                }
            }
        }
